{
  "name": "show-me-in-github",
  "displayName": "Show Me in GitHub",
  "description": "View files, commits and blame info in GitHub with keyboard shortcuts",
  "version": "0.0.3",
  "publisher": "su27",
  "author": {
    "name": "su27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/su27/show-in-github"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "github",
    "blame",
    "url",
    "commit"
  ],
  "activationEvents": [
    "onCommand:openGitHubUrl",
    "onCommand:showGitBlame",
    "onCommand:showCommit"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Show in GitHub",
      "properties": {
      }
    },
    "commands": [
      {
        "command": "openGitHubUrl",
        "title": "Show Me in GitHub: Open in GitHub"
      },
      {
        "command": "showGitBlame",
        "title": "Show Me in GitHub: Show Git Blame Information"
      },
      {
        "command": "showCommit",
        "title": "Show Me in GitHub: Show Commit Details"
      }
    ],
    "keybindings": [
      {
        "command": "openGitHubUrl",
        "key": "alt+, w",
        "mac": "alt+, w",
        "when": "(editorTextFocus || resourceScheme == 'git-commit') && (vim.mode == 'Normal' || !vim.active)"
      },
      {
        "command": "showGitBlame",
        "key": "alt+, b",
        "mac": "alt+, b",
        "when": "(editorTextFocus || resourceScheme == 'git-commit') && (vim.mode == 'Normal' || !vim.active)"
      },
      {
        "command": "showCommit",
        "key": "alt+, s",
        "mac": "alt+, s",
        "when": "(editorTextFocus || resourceScheme == 'git-commit') && (vim.mode == 'Normal' || !vim.active)"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "typescript": "^4.3.2"
  }
}