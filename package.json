{
  "name": "github-file-url",
  "displayName": "GitHub File URL",
  "description": "复制当前文件的 GitHub URL",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:github-file-url.copyGitHubUrl",
    "onCommand:github-file-url.showGitBlame",
    "onCommand:github-file-url.showCommit"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "github-file-url.copyGitHubUrl",
        "title": "复制 GitHub 文件链接"
      },
      {
        "command": "github-file-url.showGitBlame",
        "title": "显示 Git Blame 信息"
      },
      {
        "command": "github-file-url.showCommit",
        "title": "显示当前行的 Commit 信息"
      }
    ],
    "keybindings": [
      {
        "command": "github-file-url.copyGitHubUrl",
        "key": ", w",
        "mac": ", w"
      },
      {
        "command": "github-file-url.showGitBlame",
        "key": ", b l",
        "mac": ", b l"
      },
      {
        "command": "github-file-url.showCommit",
        "key": ", s",
        "mac": ", s"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "typescript": "^4.3.2"
  }
}